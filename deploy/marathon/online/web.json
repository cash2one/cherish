{
  "id": "/DEPLOY_NAME/web",
  "cmd": null,
  "cpus": 1,
  "mem": 512,
  "disk": 0,
  "instances": 1,
  "acceptedResourceRoles": [
    "online"
  ],
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
